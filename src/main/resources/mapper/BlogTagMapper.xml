<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blog.business.mapper.BlogTagMapper">
    <select id="selectByBlogId" resultType="com.blog.business.entity.BlogTag">
        select *
        from blog_tag
        where blog_id = #{blogId}
    </select>
</mapper>